<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Authors>Yari Melzani</Authors>
        <Description>Base45 encode and decode utility</Description>
        <Copyright>Yari Melzani</Copyright>
        <Company>Yari Melzani</Company>
        <AssemblyVersion>1.5</AssemblyVersion>
        <FileVersion>1.5</FileVersion>
        <TargetFrameworks>netstandard2.0;net8.0;net9.0;net10.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.5</PackageVersion>
        <Title>Base45 utility</Title>
        <PackageProjectUrl>https://github.com/iupsilon/Base45Utility</PackageProjectUrl>
        <PackageLicenseUrl>https://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/iupsilon/Base45Utility</RepositoryUrl>
        <RepositoryType>GitHub</RepositoryType>
        <PackageTags>Base45 encoding decoding</PackageTags>
        <SignAssembly>true</SignAssembly>
        <AssemblyOriginatorKeyFile>SigningKey.snk</AssemblyOriginatorKeyFile>
    </PropertyGroup>
</Project>
